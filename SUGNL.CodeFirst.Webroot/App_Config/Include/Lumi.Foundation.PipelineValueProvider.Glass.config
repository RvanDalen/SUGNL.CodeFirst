<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <mvc.valueprovider.contextitem>
        <processor type="Lumi.Foundation.PipelineValueProvider.Glass.Pipelines.ValueProvider.ResolveGlassContextItem, Lumi.Foundation.PipelineValueProvider.Glass">
          <!--Uncomment line below to disable the check if RequestedType exists in TypeConfigurations-->
          <DisableTypeConfigurationCheck>True</DisableTypeConfigurationCheck>
        </processor>
      </mvc.valueprovider.contextitem>
      <mvc.valueprovider.datasource>
        <processor type="Lumi.Foundation.PipelineValueProvider.Glass.Pipelines.ValueProvider.ResolveGlassDatasourceItem, Lumi.Foundation.PipelineValueProvider.Glass">
          <!--Uncomment line below to disable the check if RequestedType exists in TypeConfigurations-->
          <!--<DisableTypeConfigurationCheck>True</DisableTypeConfigurationCheck>-->
        </processor>
      </mvc.valueprovider.datasource>
      <mvc.valueprovider.renderingparameters>
        <processor type="Lumi.Foundation.PipelineValueProvider.Glass.Pipelines.ValueProvider.ResolveGlassRenderingParameters, Lumi.Foundation.PipelineValueProvider.Glass">
          <!--Uncomment line below to disable the check if RequestedType exists in TypeConfigurations-->
          <!--<DisableTypeConfigurationCheck>True</DisableTypeConfigurationCheck>-->
        </processor>
      </mvc.valueprovider.renderingparameters>
    </pipelines>
  </sitecore>
</configuration>